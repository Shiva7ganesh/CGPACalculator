{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/style.css",
            "dest": "public/style.css"
        },
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}